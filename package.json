{
	"name": "@ctx-core/nav",
	"version": "17.0.25",
	"description": "ctx-core nav",
	"keywords": [
		"ctx-core",
		"nav",
		"yarn"
	],
	"homepage": "https://github.com/ctx-core/nav#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/nav/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/nav/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/function": "workspace:^19.1.12",
		"@ctx-core/object": "workspace:^20.2.4",
		"@ctx-core/store": "workspace:^26.0.20",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.14",
		"@ctx-core/dev-tools": "workspace:^6.0.16",
		"@ctx-core/fontawesome": "workspace:^14.0.19",
		"@ctx-core/sass": "workspace:^11.0.17",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"modules": "index.js"
}